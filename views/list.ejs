<%- include('header') -%>
  <div class="box" id="heading">
    <h1>
      <%= listTitle %> ToDo List
    </h1>
  </div>

  <div class="box">

    <% for (let i=0; i < newListItems.length; i +=1) { %>
      <div class="item">
        <input type="checkbox">
        <p>
          <%= newListItems[i] %>
        </p>
      </div>
      <% }%>
        <form action="/" method="post">
          <input type="text" name="newItem" placeholder="Enter your task" autocomplete="off">
          <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
  </div>
<%- include('footer') -%>